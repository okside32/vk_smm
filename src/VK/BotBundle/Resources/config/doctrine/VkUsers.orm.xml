<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="VK\BotBundle\Entity\VkUsers" table="vk_users">
    <id name="id" type="string" column="id" length="150">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lastName" type="string" column="last_name" length="150" nullable="true"/>
    <field name="firstName" type="string" column="first_name" length="150" nullable="true"/>
    <field name="sex" type="boolean" column="sex" nullable="true"/>
    <field name="birthday" type="string" column="birthday" length="50" nullable="true"/>
    <field name="age" type="string" column="age" length="50" nullable="true"/>
    <field name="havePhoto" type="boolean" column="have_photo" nullable="true"/>
    <field name="country" type="string" column="country" length="150" nullable="true"/>
    <field name="city" type="string" column="city" length="150" nullable="true"/>
    <field name="platform" type="boolean" column="platform" nullable="true"/>
    <field name="lifeMain" type="string" column="life_main" length="150" nullable="true"/>
    <field name="interests" type="string" column="interests" length="450" nullable="true"/>
    <field name="phone" type="string" column="phone" length="20" nullable="true"/>
    <field name="lastSeenDate" type="string" column="last_seen_date" length="50" nullable="true"/>
    <field name="canPm" type="boolean" column="can_pm" nullable="true"/>
    <field name="canSendFriendRequest" type="boolean" column="can_send_friend_request" nullable="true"/>
    <field name="friendStatus" type="boolean" column="friend_status" nullable="true"/>
    <field name="blacklisted" type="boolean" column="blacklisted" nullable="true"/>
    <field name="dateInsert" type="datetime" column="date_insert" nullable="false"/>
  </entity>
</doctrine-mapping>
